{
  "name": "docker-runner",
  "version": "0.3.1",
  "description": "...",
  "repository": {
    "type": "git",
    "url": "git://github.com/kinda/docker-runner"
  },
  "main": "index.js",
  "scripts": {
    "release": "npm run lint && (git checkout --quiet master && test -z \"$(git status --porcelain)\" && npm version $npm_config_release_type && git push --follow-tags) || echo \"An error occured (unclean repository?)\"",
    "release-patch": "npm run release --release-type=patch",
    "release-minor": "npm run release --release-type=minor",
    "release-major": "npm run release --release-type=major",
    "lint": "eslint .",
    "start": "node index.js",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^2.2.3",
    "dockerode-promise": "^0.1.0",
    "koa": "^1.1.2",
    "koa-body": "^1.3.0",
    "lodash": "^3.10.1",
    "nomnom": "^1.8.1",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.1",
    "eslint-config-next": "^0.1.3"
  }
}
